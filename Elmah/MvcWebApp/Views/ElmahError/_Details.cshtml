@using Framework.Models;
@using Elmah.Models.Definitions;
@using Elmah.MvcWebApp.Models;
@using Framework.Mvc.Models;
@using Elmah.Models;
@using Elmah.Resx;
@model MvcItemViewModel<ElmahErrorDataModel.DefaultView>
@inject SelectListHelper selectListHelper;
@inject IUIStrings localizor;
@{
    var format_Please_Select = localizor.Get("Format_Please_Select");
}
@if (Model.Status != System.Net.HttpStatusCode.OK)
{
    <div class="row">
        <div class="text-danger">@Model.StatusMessage</div>
    </div>
}
else
{
    <input class="nt-hidden-modal-title" value="@Model!.Model!.Application" type="hidden" />
    <div class="form-group">
        <label asp-for="Model!.ErrorId" class="control-label" for="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.ErrorId))"></label>
        <input asp-for="Model!.ErrorId" readonly class="form-control " id="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.ErrorId))" name="@Model!.HtmlName(nameof(ElmahErrorDataModel.DefaultView.ErrorId))" data-nt-value="@Model!.Model!.ErrorId"/>
    </div>
    <div class="form-group">
        <label asp-for="Model!.Application" class="control-label" for="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.Application))"></label>    <div class="form-control" readonly>
        <a asp-area="" asp-controller="ElmahApplication" asp-action="Dashboard" asp-route-application="@Model!.Model!.Application">@Model!.Model!.Application_Name</a>
    </div>
    </div>
    <div class="form-group">
        <label asp-for="Model!.Host" class="control-label" for="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.Host))"></label>    <div class="form-control" readonly>
        <a asp-area="" asp-controller="ElmahHost" asp-action="Dashboard" asp-route-host="@Model!.Model!.Host">@Model!.Model!.Host_Name</a>
    </div>
    </div>
    <div class="form-group">
        <label asp-for="Model!.Type" class="control-label" for="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.Type))"></label>    <div class="form-control" readonly>
        <a asp-area="" asp-controller="ElmahType" asp-action="Dashboard" asp-route-type="@Model!.Model!.Type">@Model!.Model!.Type_Name</a>
    </div>
    </div>
    <div class="form-group">
        <label asp-for="Model!.Source" class="control-label" for="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.Source))"></label>    <div class="form-control" readonly>
        <a asp-area="" asp-controller="ElmahSource" asp-action="Dashboard" asp-route-source="@Model!.Model!.Source">@Model!.Model!.Source_Name</a>
    </div>
    </div>
    <div class="form-group">
        <label asp-for="Model!.Message" class="control-label" for="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.Message))"></label>
        <input asp-for="Model!.Message" readonly class="form-control " id="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.Message))" name="@Model!.HtmlName(nameof(ElmahErrorDataModel.DefaultView.Message))" data-nt-value="@Model!.Model!.Message"/>
    </div>
    <div class="form-group">
        <label asp-for="Model!.User" class="control-label" for="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.User))"></label>    <div class="form-control" readonly>
        <a asp-area="" asp-controller="ElmahUser" asp-action="Dashboard" asp-route-user="@Model!.Model!.User">@Model!.Model!.User_Name</a>
    </div>
    </div>
    <div class="form-group">
        <label asp-for="Model!.StatusCode" class="control-label" for="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.StatusCode))"></label>    <div class="form-control" readonly>
        <a asp-area="" asp-controller="ElmahStatusCode" asp-action="Dashboard" asp-route-statuscode="@Model!.Model!.StatusCode">@Model!.Model!.StatusCode_Name</a>
    </div>
    </div>
    <div class="form-group">
        <label asp-for="Model!.TimeUtc" class="control-label" for="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.TimeUtc))"></label>
        <input asp-for="Model!.TimeUtc" readonly class="form-control " id="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.TimeUtc))" name="@Model!.HtmlName(nameof(ElmahErrorDataModel.DefaultView.TimeUtc))" data-nt-value="@Model!.Model!.TimeUtc"/>
    </div>
    <div class="form-group">
        <label asp-for="Model!.Sequence" class="control-label" for="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.Sequence))"></label>
        <input asp-for="Model!.Sequence" readonly class="form-control " id="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.Sequence))" name="@Model!.HtmlName(nameof(ElmahErrorDataModel.DefaultView.Sequence))" data-nt-value="@Model!.Model!.Sequence"/>
    </div>
    <div class="form-group">
        <label asp-for="Model!.AllXml" class="control-label" for="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.AllXml))"></label>
        <input asp-for="Model!.AllXml" readonly class="form-control " id="@Model!.HtmlId(nameof(ElmahErrorDataModel.DefaultView.AllXml))" name="@Model!.HtmlName(nameof(ElmahErrorDataModel.DefaultView.AllXml))" data-nt-value="@Model!.Model!.AllXml"/>
    </div>
}
