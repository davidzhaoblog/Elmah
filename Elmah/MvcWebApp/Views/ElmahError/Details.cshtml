        <dd class="col-sm-10">
            <a asp-area=" asp-controller="ElmahSource" asp-action="Dashboard" asp-route-source="@Model.Source">@Model.Source_Name</a>
        </dd>
    </dl>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Message)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Message)
        </dd>
    </dl>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.User)
