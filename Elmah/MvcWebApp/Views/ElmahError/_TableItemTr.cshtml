@using Elmah.Resx;
@using Elmah.MvcWebApp.Models;
@using Framework.Models;
@using Elmah.Models;
@model MvcItemViewModel<ElmahErrorDataModel.DefaultView>
@inject IUIStrings localizor;
@{
    var routeId = Model.Model?.ErrorId;
}
<tr class="pt-0 pb-0 nt-listitem@(Model.IsCurrentItem ? " nt-created border-warning border-3": "")" data-nt-route-id="@routeId">
    @await Html.PartialAsync($"~/Views/ElmahError/_Table{Model.Template}Item.cshtml", Model)
</tr>
