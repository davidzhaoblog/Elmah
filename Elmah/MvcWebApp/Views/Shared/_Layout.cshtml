<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Elmah</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Elmah</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_NavbarRight" />
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="searchpagelist" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Search Page List
                            </a>
                            <ul class="dropdown-menu dropdown-menu-dark" aria-labelledby="searchpagelist">
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahApplication" asp-action="Index">ElmahApplication Index</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahError" asp-action="Index">ElmahError Index</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahHost" asp-action="Index">ElmahHost Index</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahSource" asp-action="Index">ElmahSource Index</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahStatusCode" asp-action="Index">ElmahStatusCode Index</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahType" asp-action="Index">ElmahType Index</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahUser" asp-action="Index">ElmahUser Index</a></li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="createformlist" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Create Form List
                            </a>
                            <ul class="dropdown-menu dropdown-menu-dark" aria-labelledby="createformlist">
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahApplication" asp-action="Create">Create ElmahApplication</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahError" asp-action="Create">Create ElmahError</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahHost" asp-action="Create">Create ElmahHost</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahSource" asp-action="Create">Create ElmahSource</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahStatusCode" asp-action="Create">Create ElmahStatusCode</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahType" asp-action="Create">Create ElmahType</a></li>
                                <li><a class="nav-link text-light" asp-area="" asp-controller="ElmahUser" asp-action="Create">Create ElmahUser</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="border-top footer text-muted">
        <div class="container-fluid">
            &copy; 2022 - Elmah - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/lib/moment.js/moment.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/font-awesome//js/all.js"></script>
    
    <script src="~/js/DDLCascading.js" asp-append-version="true"></script>
    <script src="~/js/PredefinedDateTimeRange.js" asp-append-version="true"></script>
    <script src="~/js/PaginationAndOrderBy.js" asp-append-version="true"></script>
    <script src="~/js/IndexSearch.js" asp-append-version="true"></script>
    <script src="~/js/PagedViewOption.js" asp-append-version="true"></script>

    <script src="~/js/CrudActionDialog.js"></script>
    <script src="~/js/InlineEditing.js"></script>
    <script src="~/js/BulkSelection.js"></script>
    <script src="~/js/SimpleActionConfirmationDialog.js"></script>
    <script src="~/js/BulkActions.js"></script>
    <script src="~/js/EditableList.js"></script>
    <script src="~/js/SingletonMessagePopup.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
