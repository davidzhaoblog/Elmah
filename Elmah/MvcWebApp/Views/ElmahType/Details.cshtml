@using Elmah.Models;
@using Elmah.Resx;
@model ElmahTypeModel
@inject IUIStrings localizor;
@{
    ViewData["Title"] = localizor.Get("Details");
}
<h1>@localizor.Get("Details")</h1>
<div>
    <h4>@localizor.Get("ElmahType")</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Type)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Type)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" System.Linq.Enumerable+WhereSelectListIterator`2[nTierCraft.CodeGen.Contexts.PropertyContext,System.String]>@localizor.Get("Edit")</a> |
    <a asp-action="Index">@localizor.Get("Back_to_List")</a>
</div>
